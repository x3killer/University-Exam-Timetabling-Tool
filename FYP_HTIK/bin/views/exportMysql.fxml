            </HBox>
            <HBox alignment="CENTER" prefHeight="86.0" prefWidth="791.0" spacing="20.0" />
            <HBox alignment="CENTER_RIGHT" prefHeight="86.0" prefWidth="791.0">
               <children>
                  <Button fx:id="exportButton" alignment="CENTER" mnemonicParsing="false" onAction="#onUploadButtonClick" prefHeight="55.0" prefWidth="205.0" text="Export to mySQL">
                     <HBox.margin>
                        <Insets />
                     </HBox.margin>
                  </Button>
                  <Label prefHeight="21.0" prefWidth="236.0" />
               </children>
               <VBox.margin>
