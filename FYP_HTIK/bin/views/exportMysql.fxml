<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="468.0" prefWidth="756.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.uploadtoSQL">
   <children>
      <VBox alignment="CENTER" layoutX="14.0" layoutY="75.0" prefHeight="300.0" prefWidth="791.0">
         <children>
            <HBox alignment="CENTER" prefHeight="86.0" prefWidth="791.0" spacing="20.0">
               <children>
                  <Button fx:id="finalDateSheetFilePathButton" alignment="CENTER" mnemonicParsing="false" onAction="#onFinalDateSheetPathButtonClick" prefHeight="55.0" prefWidth="230.0" text="Final Date Sheet File Path" />
